package com.static_database_access;

import java.util.Scanner;

public class StaticJDBC {

	public static void main(String[] args) {
		
		Scanner sc=  new Scanner(System.in);
		int idx = 0;
		
		while(idx != -1)
		{
			switch(idx)
			{
			   case 1:
//				   create table
				   break;
				   
			   case 2:
//				   insert a row
				   break;
				   
			   case 3:
//				   delete a row
				   break;
			}
		}
	}

}
